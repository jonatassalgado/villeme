<% cache [cache_key_for_events, params], expires: 15.minutes do %>

    <!-- HEAD -->

    <%= stylesheet_link_tag 'suit-components-grid/index.css', 'suit-utils-size/index.css', 'suit-utils-position/index.css', 'modules/Event' %>

    <!-- MAIN -->

    <%= render 'layouts/sidebar_left' %>

    <div class="col-lg-9 col-md-9 col-sm-8 col-xs-12">
      <div class="row">
        <div class="col-lg-12">

          <div id="main-content" class="Main ps-container">

            <%= render 'items_section', events: @items[:today] unless @items[:today][:preview].blank? %>

            <%= render 'items_section', events: @activities[:today] unless @activities[:today][:preview].blank? %>

            <%= render 'items_section', events: @items[:persona] unless @items[:persona][:preview].blank? %>

            <%= render 'items_section', events: @items[:neighborhood] unless @items[:neighborhood][:preview].blank? %>

            <%= render 'items_section', events: @items[:fun] unless @items[:fun][:preview].blank? %>

            <%= render 'items_section', events: @items[:education] unless @items[:education][:preview].blank? %>

            <%= render 'items_section', events: @items[:health] unless @items[:health][:preview].blank? %>


          </div>
        </div>

      </div>
    </div>


    <!-- SIDEBAR -->

    <div class="col-lg-3 col-md-3 col-sm-4 hidden-xs u-margin-95-0">
      <%= render 'sidebar' %>
    </div>


<% end %>

<!-- JS/CSS SECONDARIES -->

<%= stylesheet_link_tag 'modules/AlertCreateEvent', 'modules/AvatarPhoto', params[:controller] %>

<%= include_gon %>
<%= javascript_load_dependencies(['bLazy/blazy', 'gmap3'], sync: false) %>
<%= javascript_load_content ['gmaps-builder-user', params[:controller]], sync: false %>